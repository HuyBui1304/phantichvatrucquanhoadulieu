@article{alharmoodi_co-creation_2024,
	title = {Co-creation and critical factors for the development of an efficient public e-tourism system},
	volume = {174},
	issn = {0148-2963},
	url = {https://www.sciencedirect.com/science/article/pii/S0148296324000237},
	doi = {10.1016/j.jbusres.2024.114519},
	abstract = {This study identifies the factors that guide the adoption of a public e-tourism system resulting in value co-creation in the UAE. Integrating and comparing factors drawn from the third version of the Technology Acceptance Model (TAM3), the Technology-Task-Fit (TTF) theory, and push-to-use, an Analytic Hierarch Process (AHP) model was implemented with data collected using a structured questionnaire from purposively selected UAE e-tourism experts (N = 15) and analyzed using Microsoft Excel. The findings revealed that usefulness, convenience of use, and push-to-use were the most critical aspects for achieving an efficient public e-tourism system that allows for value co-creation in that order of ranking. The findings also suggest that computer self-efficiency is the most critical factor in effectively establishing an e-tourism system followed by government push-to-use. In conclusion, the findings demonstrate that usefulness and ease-of-use backed by computer self-efficiency, result demonstrability, and output quality are vital for the efficient adoption of a public e-tourism system resulting in value co-creation in the UAE.},
	urldate = {2024-06-05},
	journal = {Journal of Business Research},
	author = {Alharmoodi, Ahmed Abdulla and Khan, Mehmood and Mertzanis, Charilaos and Gupta, Shivam and Mikalef, Patrick and Parida, Vinit},
	month = mar,
	year = {2024},
	keywords = {Analytic Hierarchy Process, Public E-Tourism, Push-To-Use, Technology Acceptance Model-3, Technology Task Fit Model, Value Co-creation},
	pages = {114519},
}

