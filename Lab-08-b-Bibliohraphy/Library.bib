	number = {3},
	urldate = {2024-04-15},
	journal = {Indonesian Journal of Applied Research (IJAR)},
	author = {Aini, Qurotul and Sembiring, Irwan and Setiawan, Adi and Setiawan, Iwan and Rahardja, Untung},
