	keywords = {Business and management, Information systems and information technology},
	pages = {1--12},
	file = {Full Text PDF:C\:\\Users\\tungi\\Zotero\\storage\\2P8KL3JI\\Lee et al. - 2023 - Understanding continuance intention of artificial .pdf:application/pdf},
