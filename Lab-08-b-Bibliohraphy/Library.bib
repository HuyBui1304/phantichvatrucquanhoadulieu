@book{nguyen_forecasting_2024,
	title = {{FORECASTING} {RESALE} {VALUE} {OF} {CARS}: {A} {COMPARATIVE} {ANALYSIS} {OF} {LINEAR} {REGRESSION}, {RANDOM} {FOREST}, {AND} {FEEDFORWARD} {NEURAL} {NETWORKS}},
	shorttitle = {{FORECASTING} {RESALE} {VALUE} {OF} {CARS}},
	abstract = {This study investigates the effectiveness of Linear Regression (LR), Random Forest (RF), and Feedforward Neural Networks (FNN) in predicting the resale prices of used cars. Using a dataset from Kaggle comprising 558,837 observations with numerical and categorical features, we compare these models based on Mean Absolute Error (MAE), Root Mean Squared Error (RMSE), R-squared (R2), and training time. Results indicate that LR, while fast, struggled due to the dataset's lack of linearity, often predicting negative prices. RF, leveraging its tree-based structure, performed well in capturing both linear and non-linear patterns but incurred higher errors. FNN, despite requiring more training time, achieved superior performance in all metrics, highlighting its ability to discern intricate patterns in high-dimensional datasets. This research underscores the importance of model selection based on dataset characteristics and provides insights into optimizing machine learning applications in automotive resale markets.},
	author = {Nguyen, Xuan and Le, Nhat-Tung and Dung, Tran},
	month = sep,
	year = {2024},
	file = {Full Text PDF:C\:\\Users\\tungi\\Zotero\\storage\\2QIZNA5M\\Nguyen et al. - 2024 - FORECASTING RESALE VALUE OF CARS A COMPARATIVE AN.pdf:application/pdf},
}

