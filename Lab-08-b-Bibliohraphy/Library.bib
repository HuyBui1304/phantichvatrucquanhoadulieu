	keywords = {TAM, accuracy, air quality, artificial intelligence},
	pages = {209--224},
	file = {Full Text PDF:C\:\\Users\\tungi\\Zotero\\storage\\Q7Y5QHXA\\Aini et al. - 2023 - Perceived Accuracy and User Behavior Exploring th.pdf:application/pdf},
}

@article{lee_understanding_2023,
	title = {Understanding continuance intention of artificial intelligence ({AI})-enabled mobile banking applications: an extension of {AI} characteristics to an expectation confirmation model},
	volume = {10},
	copyright = {2023 The Author(s)},
	issn = {2662-9992},
	shorttitle = {Understanding continuance intention of artificial intelligence ({AI})-enabled mobile banking applications},
