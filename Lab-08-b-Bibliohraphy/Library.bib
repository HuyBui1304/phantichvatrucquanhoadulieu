	keywords = {Computer Science - Software Engineering, Computer Science - Human-Computer Interaction},
	annote = {Comment: accepted to FAccT 2024},
	file = {Wang et al. - 2024 - Investigating and Designing for Trust in AI-powere.pdf:C\:\\Users\\tungi\\Zotero\\storage\\D86Y8EQN\\Wang et al. - 2024 - Investigating and Designing for Trust in AI-powere.pdf:application/pdf},
}

